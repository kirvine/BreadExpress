<h2>Order History</h2>
<table class="striped">
  <thead>
    <tr>
      <th data-field="id">Date</th>
      <th data-field="name">Amount</th>
    </tr>
  </thead>

<% @order_history.each do |order| %>
  <tr>
    <td><%= order.date.strftime("%m-%d-%Y") %></td>
    <td><%= order.grand_total %></td>
  </tr>
<% end %>
</table>
<br />