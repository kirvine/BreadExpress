<div class="row">
  <% items.each do |item| %>
    <div class="col s4">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <%= image_tag "#{item.picture}" %>
          <p><%= item.picture %></p>
          <span class="card-title"><%= link_to item.name, item_path(item) %></span>
        </div>
      </div>
    </div>
  <% end %>
</div>